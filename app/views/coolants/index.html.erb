<p id="notice"><%= notice %></p>

<h1>Listing Coolants</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coolants.each do |coolant| %>
      <tr>
        <td><%= link_to 'Show', coolant %></td>
        <td><%= link_to 'Edit', edit_coolant_path(coolant) %></td>
        <td><%= link_to 'Destroy', coolant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coolant', new_coolant_path %>
